# include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,a[1000],flag;
    string s;
   while(cin>>n>>s){
        flag=0;
   for(int i=0;i<26;i++) a[i]={0};
    if(n==1) flag=1;
 
 
    for(int i=0;i<n;i++){
            a[s[i]-'a']++;
            if(a[s[i]-'a']>1){
                flag=1;
            }
            }
 
 
      if(flag==1) cout<<"Yes"<<endl;
else cout<<"No"<<endl;
}
}
