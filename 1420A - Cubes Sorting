#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n;
    cin>>t;
    while(t--){
        cin>>n;
        int f=0;
        vector<int>a(n);
        for(int i=0;i<n;i++) cin>>a[i];
        for(int i=0;i<n-1;i++){
            if(a[i]<=a[i+1]) f=1;
        }
        if(f==1) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
}
