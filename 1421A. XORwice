#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int a,b,c,minimum;
        cin>>a>>b;
        if(a==b) cout<<"0"<<endl;
        else{
            c=max(a,b);
           minimum=(a^c)+(b^c);
           cout<<minimum<<endl;
        }

    }

}





