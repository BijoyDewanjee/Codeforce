#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,n;
    cin>>t;
    while(t--){
    cin>>n;
    int a[n+1],b[n+1]={0},max=-1;
 
    for(int i=0;i<n;i++){
    cin>>a[i];
    }
    for(int i=0;i<n;i++){
    b[a[i]]++;
    }
    for(int i=0;i<n;i++){
    if(b[a[i]]>max){
    max=b[a[i]];
    }
    }
 
    cout<<max<<endl;
 
 
    }
}
