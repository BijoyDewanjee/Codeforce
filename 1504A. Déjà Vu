#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        string s,s1,s2,s3,s4;
        cin>>s;
        s1='a'+s;
        s2=s1;
        reverse(s1.begin(),s1.end());
        s3=s+'a';
        s4=s3;
        reverse(s3.begin(),s3.end());
        if(s1!=s2) cout<<"YES"<<endl<<s2<<endl;
        else if(s3!=s4)cout<<"YES"<<endl<<s4<<endl;
        else cout<<"NO"<<endl;

    }
}
