#include<bits/stdc++.h>
using namespace std;

int main()
{

    int t;
    cin>>t;
    while(t--){
        int n,ce=0,co=0,x,mn;
        cin>>n;
        int a[2*n];
        for(int i=0;i<2*n;i++) cin>>a[i];
            for(int i=0;i<2*n;i++){
            if(a[i]%2==0) ce++;
            else co++;
        }
        if(co==ce) cout<<"Yes"<<endl;
        else cout<<"No"<<endl;
    }
}
