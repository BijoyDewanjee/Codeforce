# include<iostream>
# include<string.h>
using namespace std;
int main()
{
    string ch;
    int i;
    cin>>ch;
    for(i=0;i<ch.size();i++)
    {
        if(ch[i]=='W' && ch[i+1]=='U' && ch[i+2]== 'B')
        {
            i=i+2;
            cout<<" ";
        }
        else
        {
            cout<<ch[i];
        }
 
    }
}
