# include<bits/stdc++.h>
using namespace std;
int main()
{
    int c=0,m=0;
    string s;
    cin>>s;
    for(int i=0;i<s.size();i++)
    {
        if (isupper(s[i])){
            m++;
        }
        else {c++;}
 
    }
    for(int i=0;i<s.size();i++){
        if(m>c) s[i]=toupper(s[i]);
        else if (m==c) s[i]=tolower(s[i]);
        else s[i] =tolower(s[i]);
 
    }
    cout<<s<<endl;
 
 
}
