# include<bits/stdc++.h>
using namespace std;
int main ()
{
    string s1,s2;
    while(cin>>s1>>s2){
            vector<int>s3;
        for(int i=0;i<s1.size();i++){
            if(s1[i]==s2[i]){
                s3.push_back(0);
            }
            else {
                s3.push_back(1);
            }
        }
        for(int i=0;i<s3.size();i++){
            cout<<s3[i];
        }
        cout<<endl;
    }
}
