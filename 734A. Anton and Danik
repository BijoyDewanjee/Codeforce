# include<bits/stdc++.h>
using namespace std;
int main()
{
   int n,count_A=0,count_D=0;
   string s;
   cin>>n>>s;
   for(int i=0;i<n;i++){
    //cin>>s[i];
 
    if(s[i]=='A'){
        count_A++;
    }
    else if(s[i]=='D')
    {
        count_D++;
    }
   }
   if(count_A>count_D)
    cout<<"Anton"<<endl;
   else if(count_D>count_A){
    cout<<"Danik"<<endl;
   }
   else{
    cout<<"Friendship"<<endl;
   }
 
    return 0;
}
